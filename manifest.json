{
  "name": "Pinboard Extension",
  "version": "0.9",
  "description": "Simple bookmark extension for pinboard",
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "icon.png",
    "popup": "popup.html"
  },
  "permissions": [
    "https://api.pinboard.in/v1/*",
    "https://api.del.icio.us/v1/*",
    "contentSettings", "tabs"
  ]
}
